{
  "name": "UI Previewer Button",
  "version": "0.3.0",
  "manifest_version": 2,
  "description": "Adds a preview button to a GitHub repo",
  "homepage_url": "http://gampleman.eu",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*",
    "https://*.github.com/*"
  ],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "options_page": "src/options/index.html"
}
