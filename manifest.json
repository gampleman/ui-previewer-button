{
  "name": "UI Previewer Button",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Adds a preview button to analytics_ui repo",
  "homepage_url": "http://gampleman.eu",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/rightscale/analytics_ui/*",
    "https://*.github.com/rightscale/analytics_ui/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/rightscale/analytics_ui/pull/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
